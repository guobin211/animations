{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "ES2015",
      "ES2016",
      "ES2017",
      "es2018",
      "es2019",
      "es2020",
      "dom"
    ],
    "paths": {
      "ngx-canvas": [
        "dist/ngx-canvas/ngx-canvas",
        "dist/ngx-canvas"
      ],
      "ngx-localforage": [
        "dist/ngx-localforage/ngx-localforage",
        "dist/ngx-localforage"
      ],
      "ngx-local": [
        "dist/ngx-local/ngx-local",
        "dist/ngx-local"
      ]
    }
  }
}
